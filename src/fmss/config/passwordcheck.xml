<?xml version="1.0" encoding="GBK"?>
<list>
	<fmss.common.util.SecurityRule>
		<name>passwordrule1</name>
		<value>[a-z]+</value>
		<message>必须包含小写字母</message>
	</fmss.common.util.SecurityRule>
	<fmss.common.util.SecurityRule>
		<name>passwordrule2</name>
		<value>[A-Z]+</value>
		<message>必须包含大写字母</message>
	</fmss.common.util.SecurityRule>
	<fmss.common.util.SecurityRule>
		<name>passwordrule3</name>
		<value>[\d]+</value>
		<message>必须包含数字</message>
	</fmss.common.util.SecurityRule>
	<fmss.common.util.SecurityRule>
		<name>passwordrule4</name>
		<value>[@#$%]+</value>
		<message>必须包含@#$%</message>
	</fmss.common.util.SecurityRule>
</list>