<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="fmss.dao.entity.UAuthSystemAdminChangeDO"
        table="U_AUTH_SYSTEM_ADMIN_CHANGE"
        dynamic-update="true"
        dynamic-insert="true"
    >
	<composite-id>
            <key-property name="userId" column="USER_ID" type="java.lang.String" />
            <key-property name="systemId" column="SYSTEM_ID" type="java.lang.String" />
    <key-property name="changeUser" column="CHANGE_USER" type="java.lang.String" />
            <key-property name="changeTime" column="CHANGE_TIME" type="java.sql.Date" />
    <key-property name="auditUser" column="AUDIT_USER" type="java.lang.String" />
            <key-property name="auditTime" column="AUDIT_TIME" type="java.sql.Date" />
    <key-property name="changeStatus" column="CHANGE_STATUS" type="java.lang.Integer" />
            <key-property name="auditStatus" column="AUDIT_STATUS" type="java.lang.Integer" />
    <key-property name="id" column="ID" type="java.lang.Long" />
            <key-property name="systemCname" column="SYSTEM_CNAME" type="java.lang.Long" />
   
   
   
    </composite-id>
    </class>
</hibernate-mapping>
